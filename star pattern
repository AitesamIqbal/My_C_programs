#include<stdio.h>
int main()
{
	for(int i=1; i<=2; i++)
	{
		printf("\t\t\t\t\t\t***********SAMI***********\n");
		printf("\t\t\t\t\t\t                          \n");
		for(int j=1; j<=2; j++)
		{
			printf(" \t\t      ***********SAMI************                *********SAMI**************\n");
			for(int k=1; k<=2; k++)
			{
				printf("\t\t      ***********SAMI************                *********SAMI**************\n");
				for(int e=1; e<=2; e++)
				{
					printf("\t\t\t\t\t\t                       \n");
					printf("\t\t\t\t\t\t*********SAMI**********\n");
				}
			}
		}
	}
}
